        .origin 2000
        .mpu 65816

        .equ str1 c00000

        .native
        .a8
        .xy16

                lda.# 3f  ; question mark
                jsr.l 00e04b  ; PUT_CHR

                lda.# .bank str1
                ldx.# str1

                jsr.l 00e04e  ; GET_CHR

                stp


        .end
