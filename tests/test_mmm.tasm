        .origin 2000
        .mpu 65816

        .native
        .a8
        .xy16

        ; Part 1: Print "a"
        lda.# 61
        jsr.l 00e04b        ; PUT_CHR

        ; Part 2: Print a small string
        lda.# .bank str1    ; bank is 00
        ldx.# str1
        jsr.l 00e04e

        stp

str1    .string0 "Print me!"


        .end

